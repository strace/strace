. "${srcdir=.}/init.sh"

try_run_prog()
{
	local rc

	"$@" > /dev/null || {
		rc=$?
		if [ $rc -eq 77 ]; then
			return 1
		else
			fail_ "$* failed with code $rc"
		fi
	}
}

while read w i
do
	try_run_prog "./$i" || continue
	run_strace -a$w -e%sched ./$i > "$EXP"
	match_diff "$LOG" "$EXP"
done << EOF
28 sched_xetaffinity
23 sched_xetparam
31 sched_rr_get_interval
33 sched_get_priority_mxx
29 sched_xetattr
22 sched_xetscheduler
14 sched_yield
EOF

echo '+++ exited with 0 +++' > "$EXP"

while read w i
do
	try_run_prog "./$i" || continue
	run_strace -a$w -e%sched ./$i > /dev/null
	match_diff "$LOG" "$EXP"
done << EOF
11 fchdir
28 futex
10 fsync
EOF

rm "$EXP"
