#!/bin/sh

# Check poll syscall decoding.

. "${srcdir=.}/init.sh"

run_prog > /dev/null
run_strace -a18 -vepoll $args > "$OUT"
match_diff "$LOG" "$OUT"

for abbrev in 0 1 2 3 4 5; do
	run_prog "./${ME_%.test}" $abbrev > /dev/null
	run_strace -a18 -epoll -s$abbrev $args > "$OUT"
	match_diff "$LOG" "$OUT"
done

rm -f "$OUT"
